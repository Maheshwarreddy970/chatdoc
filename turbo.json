{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"],
      "env": [
        "SOME_ENV_VAR"
      ]
    },
    "lint": {},
    "type-check": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "env": [
      ]
    },
    "clean": {
      "cache": false
    },
    "vercel-deploy": {
      "dependsOn": ["^build"],
      "env": [
      ],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"],
      "secrets": [
      ]
    }
  }
}

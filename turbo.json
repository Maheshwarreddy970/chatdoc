{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "globalDotEnv": ["./.env.local"],
  "globalEnv": ["NODE_ENV","DATABASE_URL"],
  "pipeline": {
    "build": {
      "dependsOn": ["^generate"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"],
      "env": [
        "DATABASE_URL"
      ]
    },
    "web#build": {
      "dependsOn": ["^db:generate","^build"],
      "outputs": [".next/**", "!.next/cache/**"],
      "env": [
        "DATABASE_URL"
      ]
    },
    "lint": {},
    "type-check": {},
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "vercel-deploy": {
      "dependsOn": ["^build"],
      "env": [
        "DATABASE_URL"
      ],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "db:migrate:deploy": {
      "outputs": []
    },
    "db:push": {
      "outputs": []
    },
    "db:seed": {
      "outputs": []
    },
    "generate": {
      "dependsOn": ["^generate"]
    }
  }
}
